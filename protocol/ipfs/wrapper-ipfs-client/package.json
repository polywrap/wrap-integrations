{
  "name": "@polywrap/ipfs-client-wasm",
  "description": "A Polywrap Wasm wrapper that implements an IPFS Client",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npx polywrap build -s image",
    "codegen": "npx polywrap codegen",
    "test": "yarn test:unit && yarn test:e2e",
    "test:ci": "yarn test:unit && yarn test:e2e",
    "test:unit": "asp --verbose",
    "test:e2e": "yarn test:e2e:codegen && jest --passWithNoTests --runInBand --verbose",
    "test:e2e:codegen": "npx polywrap codegen -m ./src/__tests__/e2e/types/polywrap.app.yaml -g ./src/__tests__/e2e/types/wrap"
  },
  "dependencies": {
    "@polywrap/wasm-as": "0.8.0",
    "as-base64": "0.2.0"
  },
  "devDependencies": {
    "axios": "0.21.4",
    "@polywrap/http-plugin-js": "0.8.0",
    "@polywrap/client-js": "0.8.0",
    "@polywrap/test-env-js": "0.8.0",
    "@as-pect/cli": "6.2.4",
    "@types/jest": "26.0.8",
    "@types/node": "16.11.64",
    "assemblyscript": "0.19.23",
    "jest": "26.6.3",
    "polywrap": "0.8.0",
    "ts-jest": "26.5.4",
    "ts-node": "10.9.1",
    "typescript": "4.6.4"
  }
}
